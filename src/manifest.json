{
  "name": "Youtube Darkmode",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Enable Youtube's new Material UI theme",
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "storage",
    "*://youtube.com/*"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "default_locale": "en",
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["cookie-setter.js"],
      "run_at" : "document_start",
      "all_frames": true
    }
  ]
}
